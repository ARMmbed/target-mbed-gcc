{
  "name": "mbed-gcc",
  "version": "1.1.0",
  "description": "base mbed build target for gcc toolchain",
  "inherits": {
      "mbed": "*"
  },
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "keywords": [
    "mbed-official",
    "gcc"
  ],
  "similarTo": [
    "mbed",
    "arm",
    "gcc"
  ],
  "config": {
    "mbed": {
      "toolchain": "gcc"
    },
    "mbed-os": {
      "stdio": {
        "default-baud": 9600
      }
    },
    "arch": {
      "arm": {}
    },
    "minar": {
      "initial_event_pool_size": 50,
      "additional_event_pools_size": 100
    }
  },
  "cmakeIncludes": [
    "coverage/coverage.cmake"
  ]
}
